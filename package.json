{
  "name": "elapsed-time-logger",
  "version": "1.1.3",
  "description": "Similiar to console.time() but returns readable elapsed time e.g Label: 1 hour 20 minutes 10.3 seconds",
  "main": "lib/elapsed-time-logger.js",
  "types": "lib/elapsed-time-logger.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --collectCoverage=true",
    "build": "rollup -c",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vltansky/elapsed-time-logger"
  },
  "bugs": {
    "url": "https://github.com/vltansky/elapsed-time-logger/issues"
  },
  "homepage": "https://github.com/vltansky/elapsed-time-logger#readme",
  "keywords": [
    "time",
    "timer",
    "elapsed",
    "span",
    "readable",
    "timing",
    "perf",
    "performance",
    "bench",
    "benchmark",
    "profiling",
    "measure",
    "seconds",
    "milliseconds",
    "nanoseconds",
    "formatted",
    "angular",
    "react",
    "node"
  ],
  "author": {
    "name": "Vlad Tansky",
    "email": "vl.tansky@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "browser-hrtime": "^1.1.7"
  },
  "devDependencies": {
    "@types/node": "^14.11.10",
    "coveralls": "^3.1.0",
    "jest": "^25.5.4",
    "prettier": "^2.0.5",
    "rollup": "^2.18.0",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "terser": "^5.0.0",
    "ts-jest": "^25.5.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3"
  }
