{
  "name": "elapsed-time-logger",
  "version": "1.1.1",
  "description": "Similiar to console.time() but returns readable elapsed time e.g Label: 1 hour 20 minutes 10.3 seconds",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --collectCoverage=true",
    "build": "tsc --module commonjs && rollup -c",
    "watch": "npm run build:cjs -- --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prerelease": "npm run test",
    "release": "dotenv release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vltansky/elapsed-time-logger"
  },
  "bugs": {
    "url": "https://github.com/vltansky/elapsed-time-logger/issues"
  },
  "homepage": "https://github.com/vltansky/elapsed-time-logger#readme",
  "keywords": [
    "time",
    "timer",
    "elapsed",
    "span",
    "readable",
    "timing",
    "perf",
    "performance",
    "bench",
    "benchmark",
    "profiling",
    "measure",
    "seconds",
    "milliseconds",
    "nanoseconds",
    "formatted",
    "angular",
    "react",
    "node"
  ],
  "author": {
    "name": "Vlad Tansky",
    "email": "vl.tansky@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "browser-hrtime": "^1.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/node": "^13.13.0",
    "coveralls": "^3.1.0",
    "dotenv-cli": "^3.1.0",
    "jest": "^25.4.0",
    "prettier": "^2.0.4",
    "release-it": "^13.5.5",
    "rollup": "^2.7.2",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "terser": "^4.6.12",
    "ts-jest": "^25.4.0",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}
