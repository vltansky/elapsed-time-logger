{
  "name": "elapsed-time-logger",
  "version": "1.1.3",
  "description": "Similiar to console.time() but returns readable elapsed time e.g Label: 1 hour 20 minutes 10.3 seconds",
  "main": "dist/elapsed-time-logger.js",
  "types": "dist/elapsed-time-logger.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest --collectCoverage=true && npm run size",
    "size": "size-limit",
    "build": "rollup -c",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "semantic-release": "semantic-release"
  },
  "size-limit": [
    {
      "limit": "2 KB",
      "path": "dist/elapsed-time-logger.js"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vltansky/elapsed-time-logger"
  },
  "bugs": {
    "url": "https://github.com/vltansky/elapsed-time-logger/issues"
  },
  "homepage": "https://github.com/vltansky/elapsed-time-logger#readme",
  "keywords": [
    "time",
    "timer",
    "elapsed",
    "span",
    "readable",
    "timing",
    "perf",
    "performance",
    "bench",
    "benchmark",
    "profiling",
    "measure",
    "seconds",
    "milliseconds",
    "nanoseconds",
    "formatted",
    "angular",
    "react",
    "node"
  ],
  "author": {
    "name": "Vlad Tansky",
    "email": "vl.tansky@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "browser-hrtime": "^1.1.8"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/node": "^14.14.31",
    "coveralls": "^3.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rollup": "^2.40.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "semantic-release": "^17.4.1",
    "size-limit": "^4.9.2",
    "terser": "^5.6.0",
    "ts-jest": "^26.5.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.2"
  }
}
